<div class='container'>
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Title:</strong>
    <%= @article.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= @article.body %>
  </p>

  <p>
    <strong>View:</strong>
    <%= @article.impressions %>
  </p>

  <%= link_to 'Edit', edit_article_path(@article), class: 'btn btn-warning' %>
  <%= link_to 'Back', articles_path, class: 'btn btn-primary' %>  
</div>
